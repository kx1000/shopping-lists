{% form_theme form
    'shopping_list/jquery.collection.html.twig'
    'shopping_list/item_form_theme.html.twig'
%}
<div class="bg-light p-5 rounded">
    {{ form_start(form) }}
        {{ form_row(form.owner) }}
        Items
        {{ form_row(form.shoppingListItems) }}
        {{ form_row(form.price) }}
        <button class="btn btn-success">
            <i class="fas fa-save"></i>
            {{ button_label|default('save') }}
        </button>
    {{ form_end(form) }}
</div>
