<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body
            data-controller="hello symfony--ux-swup--swup"
            data-theme="fade"
    >
        <main id="swup">
            {% block body %}{% endblock %}
        </main>
    </body>
</html>
